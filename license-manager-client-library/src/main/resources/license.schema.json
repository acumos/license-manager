{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "type": "object",
  "properties": {
    "modelLicenses": {
      "type": "array",
      "items": [
        {
          "type": "object",
          "required": [ "keyword", "intro", "copyright" ],
          "properties": {
            "keyword": {
              "type": "string"
            },
            "intro": {
              "type": "string"
            },
            "URL": {
              "type": "string"
            },
            "warranty": {
              "type": "string"
            },
            "copyright": {
              "type": "object",
              "required": [ "year", "company", "suffix" ],
              "properties": {
                "year": {
                  "type": "integer"
                },
                "company": {
                  "type": "string"
                },
                "suffix": {
                  "type": "string"
                }
              }
            },
            "swidTag": {
              "type": "string"
            },
            "modelId": {
              "type": "string"
            },
            "modelLimits": {
              "type": "array",
              "items": [
                {
                  "type": "object",
                  "properties": {
                    "id": {
                      "type": "string"
                    },
                    "name": {
                      "type": "string"
                    },
                    "desc": {
                      "type": "string"
                    },
                    "limit": {
                      "type": "object",
                      "properties": {
                        "type": {
                          "type": "string"
                        },
                        "value": {
                          "type": "string"
                        }
                      }
                    }
                  }
                }
              ]
            }
          }
        }
      ]
    },
    "contentLicenses": {
      "type": "array",
      "items": [
        {
          "type": "object",
          "required": [
            "keyword"
          ],
          "properties": {
            "keyword": {
              "type": "string"
            },
            "path": {
              "type": "string"
            },
            "source": {
              "type": "string"
            }
          }
        }
      ]
    }
  },
  "required": [
    "modelLicenses"
  ]
}