{
  "name": "license-profile-editor",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "prestart": "cp ../license-manager-client-library/src/main/resources/license-profile.schema.json src/assets",
    "start": "ng serve",
    "build": "ng build",
    "build:elements": "ng build --prod --output-hashing none && node build-elements.ts && cp src/web-cmp.html dist/license-profile-editor/",
    "copy:iframe": "cp ../iframe-license-editor.html dist/license-profile-editor/",
    "start:lite-server": "lite-server --baseDir='dist/license-profile-editor'",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "ci-test": "ng test --code-coverage --watch=false --browsers=ChromeHeadlessNoSandbox",
    "ci-build": "ng build --prod --no-progress"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~8.1.2",
    "@angular/cdk": "~8.1.1",
    "@angular/common": "~8.1.2",
    "@angular/compiler": "~8.1.2",
    "@angular/core": "~8.1.2",
    "@angular/elements": "^8.1.2",
    "@angular/flex-layout": "^8.0.0-beta.26",
    "@angular/forms": "~8.1.2",
    "@angular/material": "^8.1.1",
    "@angular/platform-browser": "~8.1.2",
    "@angular/platform-browser-dynamic": "~8.1.2",
    "@angular/router": "~8.1.2",
    "@earlyster/angular6-json-schema-form": "^8.1.1-alpha",
    "@webcomponents/custom-elements": "^1.2.4",
    "ajv": "^6.10.2",
    "document-register-element": "^1.7.2",
    "hammerjs": "^2.0.8",
    "json2yaml": "^1.1.0",
    "lodash-es": "^4.17.14",
    "rxjs": "~6.5.2",
    "rxjs-compat": "^6.5.2",
    "scss": "^0.2.4",
    "scss-loader": "0.0.1",
    "tslib": "^1.9.0",
    "zone.js": "~0.9.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.801.0",
    "@angular/cli": "~8.1.0",
    "@angular/compiler-cli": "~8.1.2",
    "@angular/language-service": "~8.1.2",
    "@types/jasmine": "~3.3.8",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "~8.9.4",
    "codelyzer": "^5.0.0",
    "concat": "^1.0.3",
    "fs-extra": "^8.1.0",
    "jasmine-core": "~3.4.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~4.1.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.1",
    "karma-jasmine": "~2.0.1",
    "karma-jasmine-html-reporter": "^1.4.0",
    "lite-server": "^2.5.3",
    "protractor": "~5.4.0",
    "ts-node": "~7.0.0",
    "tslint": "~5.15.0",
    "typescript": "~3.4.3"
  }
